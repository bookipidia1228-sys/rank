{
    "cleanUrls": true,
    "redirects": [
      { "source": "/", "destination": "/rank", "permanent": true }
    ],
    "rewrites": [
      { "source": "/rank", "destination": "/rank/index.html" },
      { "source": "/rank/", "destination": "/rank/index.html" },
      { "source": "/rank/:year", "destination": "/rank/index.html" },
      { "source": "/rank/:year/:group", "destination": "/rank/index.html" },
      { "source": "/rank/:year/:group/:type/:id", "destination": "/rank/index.html" }
    ]
  }
  
