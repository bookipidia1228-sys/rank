{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "^/(\\d{4})$", "destination": "/" },
    { "source": "^/(\\d{4})/([^/]+)$", "destination": "/" },
    { "source": "^/(\\d{4})/([^/]+)/([^/]+)$", "destination": "/" }
  ]
}
